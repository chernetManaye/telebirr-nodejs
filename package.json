{
  "name": "telebirr-nodejs",
  "version": "1.0.8",
  "description": "Unofficial Telebirr Node.js SDK.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "nanoid": "^5.1.6"
  },
  "keywords": [
    "telebirr",
    "ethiopia",
    "payment",
    "fintech",
    "nodejs"
  ],
  "author": "chernet manaye",
  "license": "MIT"
}
